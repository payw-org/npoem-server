type User {
    id: ID!,
    nickname: String!,
}

extend type Query {
    users: [User]
    user(nickname: String!): User
    isPlayedToday: Boolean
}

extend type Mutation {
    createUser(userSaveDto: UserSaveRequestDto!): String!
    updateNickname(token: String!, nickname: String!): String!
}

input UserSaveRequestDto {
}